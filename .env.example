# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google OAuth (for Calendar integration)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Anthropic API (for Email Ghostwriter)
VITE_ANTHROPIC_API_KEY=your-anthropic-api-key

# Monday.com Integration
VITE_MONDAY_API_TOKEN=your-monday-api-token
VITE_MONDAY_GEODE_BOARD_ID=your-geode-board-id
VITE_MONDAY_PAYMENTS_BOARD_ID=your-payments-board-id

# Slack Integration
VITE_SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
VITE_SLACK_SIGNING_SECRET=your-slack-signing-secret
VITE_SLACK_DEFAULT_CHANNEL_ID=your-default-channel-id

# Feature Flags
VITE_ENABLE_VOICE=true
VITE_ENABLE_MEETING_MONITORING=false
